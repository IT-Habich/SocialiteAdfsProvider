        "Laravel Socialite",
        "ADFS",
        "Microsoft Active Directory Federation Services",
        "OAuth",
        "OAuth2",
        "SSO",
        "Single-Sign-On"
    ],
    "authors": [
        {
            "name": "IT-Habich - Frederic Roggon",
            "email": "packagist@it-habich.de",
            "homepage": "https://it-habich.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeAdminDe\\SocialiteAdfsProvider\\": "src/"
        }
    },
    "extra": {
